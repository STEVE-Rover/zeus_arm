<?xml version='1.0'?>
<launch>
  <include file="$(find steve_control)/launch/multi_master.launch" />

  <!-- Launch teleop script -->
  <node name="teleop_node" pkg="zeus_arm" type="pos_teleop_real.py" output="screen" />

  <!-- Launch arm node -->
  <node name="arm_node" pkg="zeus_arm" type="arm_node_real.py" output="screen" />

  <!-- Launch rosserial_arduino node -->
  <node name="arduino_serial" pkg="rosserial_arduino" type="serial_node.py">
    <param name="port" value="/dev/ttyACM0" />
  </node>

</launch>
